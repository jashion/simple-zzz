{
  "initial": "",
  "serviceList": [
    {
      "name": "wechat",
      "path": "/wechat",
      "realizeFile": "oauth",
      "realizeFunction": "wechatEnableDevMode",
      "method": "get",
      "desc": "开启微信开发模式"
    },
    {
      "name": "oauthLogin",
      "path": "/:thirdParty/login",
      "realizeFile": "oauth",
      "realizeFunction": "oauthLogin",
      "method": "post",
      "desc": "第三方登陆"
    },
    {
      "name": "webOauthLogin",
      "path": "/web/:thirdParty/login",
      "realizeFile": "oauth",
      "realizeFunction": "webOauthLogin",
      "method": "get",
      "desc": "网页第三方登陆"
    },
    {
      "name": "oauthCallback",
      "path": "/:thirdParty/callback",
      "realizeFile": "oauth",
      "realizeFunction": "oauthCallback",
      "method": "get",
      "desc": "第三登陆回调"
    }
  ]
}